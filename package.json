{
  "name": "flight-tracker-opensky",
  "private": true,
  "version": "0.1.0",
  "description": "Free-ish flight tracker using OpenSky (live ADS-B) + React map UI",
  "scripts": {
    "dev": "npm-run-all -p dev:server dev:web",
    "dev:server": "npm --prefix server run dev",
    "dev:web": "npm --prefix web run dev",
    "build": "npm --prefix web run build",
    "start": "npm --prefix server start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}